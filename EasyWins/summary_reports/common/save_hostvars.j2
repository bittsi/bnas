{# 
	Iterate over hostvars for a particular host and remove sensitive data
	Write out remaining data as key-/value-pairs in json-format (can be used to dump in yaml-file)
#}
{% for key,value in allhosts[inventory_hostname]|dictsort %}
{% if key != "cli" and not('vault' in key) %}
{{key}}: {{value |to_json}}
{% endif %}
{% endfor %}

